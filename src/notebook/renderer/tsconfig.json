{
  "compilerOptions": {
    "module": "esnext",
    "target": "ES2019",
    "lib": ["ES2019", "dom"],
    "jsx": "react",
    "types": ["node", "webpack-env", "vscode-notebook-renderer"],
    "moduleResolution": "node",
    "strict": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "rootDir": ".",
    "importHelpers": true,
    "sourceMap": true
  }
}
